        /* Custom Select */
        .select {
        position: relative;
        display: flex;
        width: 300px;
        height: 2em;
        line-height: 2;
        background: #2c3e50;
        overflow: hidden;
        border-radius: .25em;
        }
        select {
        flex: 1;
        padding: 0 .5em;
        color: #fff;
        cursor: pointer;
